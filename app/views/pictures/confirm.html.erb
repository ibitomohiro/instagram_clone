<h3>投稿の確認</h3>
<%= form_with(model:@picture, local: true) do |f| %>
<div class="row">
  <div class="col-md-6  col-md-offset-3">
    <%= iamge_tag(@picture,image.middle.url) if picture.iamge.url %>
    <%= f.hidden_field :image_cache %>
    <% f.hidden_field :content %>
    <%= f.submit "投稿" %>
  </div>
</div>
<% end %>